<IfModule deflate_module>
	    SetOutputFilter DEFLATE
		AddOutputFilter DEFLATE text/html
		AddOutputFilter DEFLATE text/css
		AddOutputFilter DEFLATE text/javascript
		AddOutputFilter DEFLATE image/gif
		AddOutputFilter DEFLATE image/jpeg
		AddOutputFilter DEFLATE image/png
		AddOutputFilter DEFLATE application/javascript
</IfModule>

<IfModule expires_module>
	ExpiresActive On
	ExpiresByType text/html "access plus 2 weeks"
	ExpiresByType text/css "access plus 2 weeks"
	ExpiresByType text/javascript "access plus 2 weeks"
	ExpiresByType image/gif "access plus 2 weeks"
	ExpiresByType image/jpeg "access plus 2 weeks"
	ExpiresByType image/png "access plus 2 weeks"
	ExpiresByType application/javascript "access plus 2 weeks"
</IfModule>